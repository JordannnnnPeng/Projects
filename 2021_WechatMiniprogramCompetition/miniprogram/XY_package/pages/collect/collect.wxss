/* XY_package/pages/collect/collect.wxss */
.container .box1 {
  display: flex;
  height: 200rpx;
  background-color: #f7f3e9;
  margin-bottom: 20rpx;
  margin-top: 20rpx;
  align-items: center;
  border-radius: 1em;
}
.box1 .pic {
  width: 230rpx;
  height: 180rpx;
  margin-left: auto;
  border-radius: 1em;
}
.container .words {
  justify-content: space-between;
  /* border:solid 1rpx grey; */
  margin-left: 40rpx;
}
.container .words .space {
  margin-top: 1em;
  margin-bottom: 1em;
}
.container .words .words_up {
  /* font-size: small; */
  font-style: bold;
  color: black;
}
.container .words .words_down {
  font-size: small;
  color: black;
}
